; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Bubble"
run/main_scene="res://scenes/text_screen.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/bg_color=Color(0.231373, 0.239216, 0.32549, 1)
boot_splash/image="res://assets/bootsplashanim.png"
boot_splash/use_filter=false
config/icon="res://icon.svg"
boot_splash/minimum_display_time=10

[autoload]

SceneTransition="*res://scenes/scene_transition.tscn"
Util="*res://scenes/util.tscn"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=160
window/size/viewport_height=144
window/size/window_width_override=1280
window/size/window_height_override=1152
window/stretch/mode="viewport"

[editor]

movie_writer/movie_file="/Users/johnlemuel/Godot/bubble/Canary Demo Video.avi"

[file_customization]

folder_colors={
"res://assets/": "red",
"res://scenes/": "blue",
"res://scripts/": "green"
}

[global_group]

enemy=""
player=""
pickup=""
override_camera=""

